@model SLNClinica.Models.Medico

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
    <h1>Registrar Medico</h1>
    @using (Html.BeginForm("Create", "Medico", FormMethod.Post))
    {
            <p>
            @Html.LabelFor(m=> m.Nombre)
            @Html.TextBoxFor(m=>m.Nombre)
            @Html.ValidationMessageFor(m=> m.Nombre)
            </p>

            <p>
            @Html.LabelFor(m=> m.Apellido)
            @Html.TextBoxFor(m=>m.Apellido)
            @Html.ValidationMessageFor(m=> m.Apellido)
            </p>

            <p>
            @Html.LabelFor(m=> m.Matricula)
            @Html.TextBoxFor(m=>m.Matricula)
            @Html.ValidationMessageFor(m=> m.Matricula)
            </p>

            <input type="submit" action="Create" />
        @Html.ActionLink("Volver","Index", "Medico")

    }

</body>
</html>
